{
  "eula": "",
  "vendor": "InsightCubes",
  "license": "",
  "id": "InsightCubes.sankeychart",
  "icon": "",
  "version": "1.0.0",
  "supportsMobile": true,
  "supportsBookmark": true,
  "supportsLinkedAnalysisFilterOnSelection": true,
  "name": "Sankey Chart",
  "newInstancePrefix": "SankeyChart",
  "description": "Sankey chart visualization widget",
  "webcomponents": [
    {
      "kind": "main",
      "tag": "insightcubes-sankeychart",
      "url": "https://insightcubes.com/sacwidgets/SAPWidgets/sankeyChart/sankeyChartMain.js",
      "integrity": "",
      "ignoreIntegrity": true
    },
    {
      "kind": "styling",
      "tag": "insightcubes-sankeychart-styling",
      "url": "https://insightcubes.com/sacwidgets/SAPWidgets/sankeyChart/sankeyChartStyling.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "width": {
      "type": "integer",
      "default": 600
    },
    "height": {
      "type": "integer",
      "default": 320
    },
    "depth0Settings": {
      "type": "stylingSetting",
      "default": {"itemColor": "#348B26", "lineOpacity": 0.6}
    },
    "depth1Settings": {
      "type": "stylingSetting",
      "default": {"itemColor": "#4FB81C", "lineOpacity": 0.4}
    },
    "depth2Settings": {
      "type": "stylingSetting",
      "default": {"itemColor": "#93C939", "lineOpacity": 0.2}
    },
    "depth3Settings": {
      "type": "stylingSetting",
      "default": {"itemColor": "#BCDC50", "lineOpacity": 0.1}
    }
  },
  "events": {},
  "methods": {
		"getDataSource": {
			"parameters": [],
			"body": "return this.dataBindings.getDataBinding('dataBinding').getDataSource();",
			"returnType": "DataSource"
		}
  },
  "types": {
    "stylingSetting": {
      "description": "type for styling settings",
      "properties": [
        {
          "name": "itemColor",
          "type": "string",
          "description": "item cloor"
        },
        {
          "name": "lineOpacity",
          "type": "number",
          "description": "line opacity"
        }
      ]
    }
  },
  "dataBindings": {
    "dataBinding": {
      "feeds": [
        {
          "id": "dimensions",
          "description": "Dimensions",
          "type": "dimension"
        },
        {
          "id": "measures",
          "description": "Measures",
          "type": "mainStructureMember"
        }
      ]
    }
  }
}
