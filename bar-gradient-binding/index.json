{
  "eula": "",
  "vendor": "InsightCubes",
  "license": "",
  "id": "com.insightcubes.sac.bargradient",
  "icon": "data:image/png;base64,",
  "version": "1.1.0",
  "supportsMobile": true,
  "name": "Bar Gradient",
  "newInstancePrefix": "BarGradient",
  "description": "A customizable bar chart widget with gradient effects",
  "webcomponents": [
    {
      "kind": "main",
      "tag": "com-insightcubes-sac-bargradient",
      "url": "https://cdn.jsdelivr.net/gh/kamalmerhebi/SAPWidgets@main/bar-gradient-binding/main.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "width": {
      "type": "integer",
      "default": 600,
      "description": "Width of the chart in pixels"
    },
    "height": {
      "type": "integer",
      "default": 420,
      "description": "Height of the chart in pixels"
    },
    "title": {
      "type": "string",
      "default": "Bar Gradient Chart",
      "description": "Main title of the chart"
    },
    "titleFontSize": {
      "type": "integer",
      "default": 18,
      "description": "Font size of the chart title in pixels"
    },
    "titleColor": {
      "type": "string",
      "default": "#333333",
      "description": "Color of the chart title in hex format"
    },
    "gradientStartColor": {
      "type": "string",
      "default": "#83bff6",
      "description": "Starting color of the bar gradient in hex format"
    },
    "gradientMiddleColor": {
      "type": "string",
      "default": "#188df0",
      "description": "Middle color of the bar gradient in hex format"
    },
    "gradientEndColor": {
      "type": "string",
      "default": "#188df0",
      "description": "End color of the bar gradient in hex format"
    },
    "barWidth": {
      "type": "integer",
      "default": 40,
      "description": "Width of each bar in pixels"
    },
    "barGap": {
      "type": "integer",
      "default": 30,
      "description": "Gap between bars in percentage"
    },
    "axisLabelFormat": {
      "type": "string",
      "default": "{value} Million",
      "description": "Format string for axis labels"
    },
    "axisLabelColor": {
      "type": "string",
      "default": "#999999",
      "description": "Color of axis labels in hex format"
    },
    "enableZoom": {
      "type": "boolean",
      "default": true,
      "description": "Enable or disable chart zooming functionality"
    }
  },
  "methods": {
    "getTitle": {
      "returnType": "string",
      "description": "Returns the current chart title",
      "body": "return this.title;"
    },
    "setTitle": {
      "parameters": [
        {
          "name": "title",
          "type": "string",
          "description": "New title text to be set for the chart"
        }
      ],
      "description": "Sets a new title for the chart and updates the visualization"
    }
  },
  "events": {},
  "dataBindings": {
    "myDataBinding": {
      "feeds": [
        {
          "id": "dimensions",
          "description": "Dimensions",
          "type": "dimension"
        },
        {
          "id": "measures",
          "description": "Measures",
          "type": "mainStructureMember"
        }
      ]
    }
  }
}