{
  "eula": "",
  "vendor": "InsightCubes",
  "license": "",
  "id": "insightcubes.echarts.bargradientbinding",
  "icon": "data:image/png;base64,",
  "version": "1.0.0",
  "supportsMobile": true,
  "supportsExport": true,
  "name": "ECharts Bar Gradient Binding",
  "newInstancePrefix": "EChartsBarGradientBinding",
  "description": "ECharts Bar Gradient with Data Binding",
  "webcomponents": [
    {
      "kind": "main",
      "tag": "insightcubes-echarts-bargradientbinding",
      "url": "https://cdn.jsdelivr.net/gh/kamalmerhebi/SAPWidgets@main/bar-gradient-binding/main.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "width": {
      "type": "integer",
      "default": 600
    },
    "height": {
      "type": "integer",
      "default": 420
    },
    "title": {
      "type": "string",
      "description": "Chart title",
      "default": "Bar Chart with Gradient"
    }
  },
  "methods": {
    "setTitle": {
      "description": "Sets the chart title",
      "parameters": [
        {
          "name": "title",
          "type": "string",
          "description": "The new title"
        }
      ]
    },
    "refresh": {
      "description": "Refreshes the chart"
    }
  },
  "events": {
    "onClick": {
      "description": "Called when a bar is clicked"
    }
  },
  "dataBindings": {
    "myDataBinding": {
      "feeds": [
        {
          "id": "dimensions",
          "description": "Dimensions",
          "type": "dimension"
        },
        {
          "id": "measures",
          "description": "Measures",
          "type": "mainStructureMember"
        }
      ]
    }
  }
}