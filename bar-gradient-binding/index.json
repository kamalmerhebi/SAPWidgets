{
  "eula": "",
  "vendor": "SAP",
  "license": "",
  "id": "com.sap.sac.sample.echarts.bargradientbindingdemo",
  "icon": "data:image/png;base64,",
  "version": "1.1.0",
  "supportsMobile": true,
  "name": "ECharts BarGradientBinding",
  "newInstancePrefix": "EChartBarGradientBinding",
  "description": "A sample custom widget wrapped EChart BarGradientBinding",
  "webcomponents": [
    {
      "kind": "main",
      "tag": "com-sap-sample-echarts-bar-gradient-binding",
      "url": "https://cdn.jsdelivr.net/gh/kamalmerhebi/SAPWidgets@main/bar-gradient-binding/main.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "width": {
      "type": "integer",
      "default": 600
    },
    "height": {
      "type": "integer",
      "default": 420
    },
    "title": {
      "type": "string",
      "default": "Bar Gradient Chart"
    },
    "titleFontSize": {
      "type": "integer",
      "default": 18
    },
    "titleColor": {
      "type": "string",
      "default": "#333333"
    },
    "gradientStartColor": {
      "type": "string",
      "default": "#83bff6"
    },
    "gradientMiddleColor": {
      "type": "string",
      "default": "#188df0"
    },
    "gradientEndColor": {
      "type": "string",
      "default": "#188df0"
    },
    "barWidth": {
      "type": "integer",
      "default": 40
    },
    "barGap": {
      "type": "integer",
      "default": 30
    },
    "axisLabelFormat": {
      "type": "string",
      "default": "{value} Million"
    },
    "axisLabelColor": {
      "type": "string",
      "default": "#999999"
    },
    "enableZoom": {
      "type": "boolean",
      "default": true
    }
  },
  "methods": {
    "getTitle": {
      "returnType": "string",
      "description": "Returns the current chart title"
    },
    "setTitle": {
      "parameters": [
        {
          "name": "title",
          "type": "string",
          "description": "New chart title"
        }
      ],
      "description": "Sets the chart title"
    }
  },
  "events": {},
  "dataBindings": {
    "myDataBinding": {
      "feeds": [
        {
          "id": "dimensions",
          "description": "Dimensions",
          "type": "dimension"
        },
        {
          "id": "measures",
          "description": "Measures",
          "type": "mainStructureMember"
        }
      ]
    }
  }
}